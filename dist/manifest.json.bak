{
  "description": "",
  "manifest_version": 3,
  "name": "MSN Article Sourcer",
  "version": "1.0.0",
  "homepage_url": "https://github.com/parkersprouse",
  "icons": {
    "48": "img/logo.svg",
    "96": "img/logo.svg"
  },
  "content_scripts": [
    {
      "matches": ["*://*.msn.com/*"],
      "js": ["popup.js"]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "action": {
    "default_icon": "img/logo.svg",
    "default_popup": "popup.html",
    "default_title": "MSN Article Sourcer"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "parker@parkersprouse.me",
      "strict_min_version": "110.0"
    }
  }
}
